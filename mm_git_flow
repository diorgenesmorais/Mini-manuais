/**
 * Git flow
 * Pattern branches:
 * master e develop
 */

// iniciar o git flow
git flow init

// iniciar uma feature
git flow feature start nome_da_feature

// finalizar uma feature (faz um merge automaticamente com develop e exclui a feature)
git flow feature finish nome_da_feature

// publicar uma feature
git flow feature publish nome_da_feature

// pegar uma feature do repositÃ³rio remoto
git flow feature track nome_da_feature

// listar as features (list is optional)
git flow feature list
